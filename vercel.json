{
    "version": 2,
    "functions": {
        "api/index.js": {
        "maxDuration": 30
        }
    },
    "builds": [
            {
                "src": "app.js",
                "use": "@vercel/node"
            }
        ],
    "routes":[
        { 
            "src": "/(.*)", 
            "dest": "app.js" 
        }
    ] 
}